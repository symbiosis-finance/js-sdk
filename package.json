{
    "name": "symbiosis-js-sdk",
    "version": "3.7.12",
    "typings": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/symbiosis-finance/js-sdk.git"
    },
    "bugs": {
        "url": "https://github.com/symbiosis-finance/js-sdk/issues"
    },
    "type": "module",
    "exports": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
    },
    "scripts": {
        "build": "rm -fr ./dist && tsc && fix-esm-import-path dist/index.js",
        "start": "tsc-watch --onSuccess 'fix-esm-import-path dist/index.js'",
        "prettier": "prettier --check --ignore-unknown src",
        "prettier:fix": "prettier --write --ignore-unknown src test",
        "build-contracts": "typechain --discriminate-types --target ethers-v5 --out-dir src/crosschain/contracts './src/crosschain/abis/*.json' && npm run prettier:fix",
        "build-config-cache": "tsx ./src/crosschain/config/cache/cmd/build.ts && prettier --write --ignore-unknown src/crosschain/config/cache",
        "test": "vitest test/crosschain",
        "prepare": "npm run build-contracts && npm run build"
    },
    "dependencies": {
        "@bitcoinerlab/secp256k1": "^1.1.1",
        "@chainflip/sdk": "^1.11.2",
        "@dedust/sdk": "^0.8.7",
        "@ethersproject/abi": "^5.8.0",
        "@ethersproject/address": "^5.8.0",
        "@ethersproject/bignumber": "^5.8.0",
        "@ethersproject/bytes": "^5.8.0",
        "@ethersproject/constants": "^5.8.0",
        "@ethersproject/contracts": "^5.8.0",
        "@ethersproject/keccak256": "^5.8.0",
        "@ethersproject/providers": "^5.8.0",
        "@ethersproject/solidity": "^5.8.0",
        "@ethersproject/units": "^5.8.0",
        "@opentelemetry/api": "^1.9.0",
        "@orbs-network/ton-access": "^2.3.3",
        "@raydium-io/raydium-sdk-v2": "0.1.106-alpha",
        "@solana/addresses": "^2.0.0",
        "@solana/spl-token": "^0.4.0",
        "@solana/web3.js": "^1.98.0",
        "@ston-fi/api": "^0.29.0",
        "@ston-fi/sdk": "^2.7.0",
        "@ton/core": "^0.59.0",
        "@ton/crypto": "3.3.0",
        "@ton/ton": "~15.1.0",
        "@uniswap/sdk-core": "3.1.0",
        "@uniswap/v3-core": "^1.0.1",
        "@uniswap/v3-sdk": "3.27.0",
        "big.js": "^5.2.2",
        "bignumber.js": "^9.0.1",
        "bitcoin-address-validation": "^2.2.3",
        "bitcoinjs-lib": "^6.1.5",
        "crypto-js": "^4.2.0",
        "decimal.js-light": "^2.5.0",
        "ethers": "^5.8.0",
        "isomorphic-unfetch": "^3.1.0",
        "jsbi": "^3.1.1",
        "lodash.flatmap": "^4.5.0",
        "pino": "^8.0.0",
        "pino-pretty": "^8.0.0",
        "prom-client": "^15.1.3",
        "tiny-invariant": "^1.1.0",
        "tiny-warning": "^1.0.3",
        "toformat": "^2.0.0",
        "tronweb": "^5.3.0"
    },
    "devDependencies": {
        "@typechain/ethers-v5": "^11.1.2",
        "@types/big.js": "^4.0.5",
        "@types/bs58": "^4.0.0",
        "@types/crypto-js": "^4.2.1",
        "@types/jest": "^27.0.3",
        "@types/lodash.flatmap": "^4.5.6",
        "@types/prettier": "2.7.3",
        "@typescript-eslint/eslint-plugin": "^8.52.0",
        "@typescript-eslint/parser": "^8.52.0",
        "babel-plugin-transform-jsbi-to-bigint": "^1.3.1",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.4",
        "fix-esm-import-path": "^1.10.3",
        "nock": "^14.0.10",
        "prettier": "3.7.4",
        "prettier-plugin-organize-imports": "^4.3.0",
        "prom-client": "^15.1.3",
        "ts-node": "^10.9.1",
        "tsc-watch": "^7.2.0",
        "tsx": "^4.21.0",
        "typechain": "^8.3.2",
        "typescript": "^5.0.4",
        "vitest": "^4.0.16"
    },
    "engines": {
        "node": ">=18"
    }
}
