{
    "name": "symbiosis-js-sdk",
    "version": "3.6.7",
    "type": "module",
    "exports": {
        "types": {
            ".": "./dist/index.d.ts"
        },
        "import": {
            ".": "./src/index.ts",
            "./*.ts": "./src/*.ts"
        },
        "require": {
            ".": "./dist/index.cjs.js"
        }
    },
    "files": [
        "dist/*"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/symbiosis-finance/js-sdk.git"
    },
    "bugs": {
        "url": "https://github.com/symbiosis-finance/js-sdk/issues"
    },
    "scripts": {
        "build": "tsup",
        "start": "tsup --watch",
        "prettier": "prettier --check --ignore-unknown src",
        "prettier:fix": "prettier --write --ignore-unknown src test",
        "prepublishOnly": "tsc --noEmit && tsup",
        "build-contracts": "typechain --target ethers-v5 --out-dir src/crosschain/contracts --node16-modules './src/crosschain/abis/*.json' && npm run prettier:fix",
        "build-config-cache": "ts-node -O '{\"module\":\"commonjs\"}' --files ./src/crosschain/config/cache/cmd/build.ts && prettier --write --ignore-unknown src/crosschain/config/cache",
        "test": "vitest test/crosschain"
    },
    "dependencies": {
        "@bitcoinerlab/secp256k1": "^1.1.1",
        "@chainflip/sdk": "^1.10.0",
        "@dedust/sdk": "^0.8.7",
        "@orbs-network/ton-access": "^2.3.3",
        "@raydium-io/raydium-sdk-v2": "0.1.106-alpha",
        "@solana/addresses": "^2.0.0",
        "@solana/spl-token": "^0.4.0",
        "@solana/web3.js": "^1.98.0",
        "@ston-fi/api": "0.23.0",
        "@ston-fi/sdk": "^2.4.0",
        "@ton/core": "^0.59.0",
        "@ton/crypto": "3.3.0",
        "@ton/ton": "~15.1.0",
        "@uniswap/sdk-core": "^7.7.2",
        "@uniswap/v3-sdk": "^3.25.2",
        "big.js": "^5.2.2",
        "bignumber.js": "^9.0.1",
        "bitcoin-address-validation": "^2.2.3",
        "bitcoinjs-lib": "^6.1.5",
        "decimal.js-light": "github:arjunyel/decimal.js-light#fix-types-2",
        "ethers": "^5.8.0",
        "jsbi": "github:symbiosis-finance/jsbi#v3.2.5+symbiosis5",
        "lodash.flatmap": "^4.5.0",
        "tiny-invariant": "^1.1.0",
        "tiny-warning": "^1.0.3",
        "toformat": "^2.0.0",
        "tronweb": "^5.3.0",
        "pino": "^8.0.0",
        "pino-pretty": "^8.0.0"
    },
    "devDependencies": {
        "@symbiosis-finance/depository": "github:symbiosis-finance/depository#v0.5.0",
        "@typechain/ethers-v5": "^11.1.2",
        "@types/big.js": "^4.0.5",
        "@types/bs58": "^4.0.0",
        "@types/crypto-js": "^4.2.1",
        "@types/jest": "^27.0.3",
        "@types/lodash.flatmap": "^4.5.6",
        "@types/prettier": "2.7.3",
        "@typescript-eslint/eslint-plugin": "^7.6.0",
        "@typescript-eslint/parser": "^7.6.0",
        "babel-plugin-transform-jsbi-to-bigint": "^1.3.1",
        "crypto-js": "^4.2.0",
        "eslint": "^8.5.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "ethers": "^5.2.0",
        "nock": "^14.0.10",
        "prettier": "^3.6.2",
        "prom-client": "^15.1.3",
        "ts-node": "^10.9.1",
        "tsup": "^8.5.0",
        "typechain": "^8.3.2",
        "typescript": "^5.0.4",
        "vitest": "^0.34.2"
    },
    "engines": {
        "node": ">=20"
    }
}
